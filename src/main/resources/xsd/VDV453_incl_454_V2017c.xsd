				<xsd:annotation>
					<xsd:documentation>Veraltet. Soll nicht benutzt werden.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FahrplanVersionID" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Veraltet. Soll nicht benutzt werden.</xsd:documentation>
				</xsd:annotation>
