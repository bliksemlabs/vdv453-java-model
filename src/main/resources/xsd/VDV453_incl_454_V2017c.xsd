			<xsd:element name="FoFahrzeugeZustaende">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FoFahrzeugZustand" type="FoFahrzeugZustandType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoFahrzeugZustandType">
		<xsd:sequence>
			<xsd:element name="FoFahrzeugIDREF" type="FoFahrzeugIDREFType"/>
			<xsd:element name="FoZustand" type="FoZustandType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>============================== Formation Fahrt Abschnitt Belegung =====================================</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="FoFahrzeugBelegungFahrtAbschnittType">
		<xsd:sequence>
			<xsd:element name="FoAbschnitt" type="FahrtStartEndeType"/>
			<xsd:element name="FoFahrzeugeBelegungen">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FoFahrzeugBelegung" type="FoFahrzeugBelegungType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoFahrzeugBelegungType">
		<xsd:sequence>
			<xsd:element name="FoFahrzeugIDREF" type="FoFahrzeugIDREFType"/>
			<xsd:element name="FoBelegungProzentual" type="FoBelegungProzentualType" minOccurs="0"/>
			<xsd:element name="FoReisegruppen" type="FoReisegruppenType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoReisegruppenType">
		<xsd:sequence>
			<xsd:element name="FoReisegruppeVorhanden" type="FoReisegruppeVorhandenType"/>
			<xsd:element name="FoReisegruppenNamen" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FoReisegruppenName" type="FoReisegruppenNameType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="FoReisegruppeVorhandenType">
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:simpleType name="FoBelegungProzentualType">
		<xsd:restriction base="xsd:positiveInteger"/>
	</xsd:simpleType>
	<xsd:complexType name="FoReisegruppenNameType">
		<xsd:sequence>
			<xsd:element name="FoName" type="FoNameType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="FoNameType">
		<xsd:restriction base="FoStringType"/>
	</xsd:simpleType>
	<xsd:annotation>
		<xsd:documentation>============================== Formation am Halt ===================================================</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="FoHaltType">
		<xsd:sequence>
			<xsd:element name="HaltID" type="HaltIDType"/>
			<xsd:element name="Abfahrtszeit" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="Ankunftszeit" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="StoerungsInfo" type="StoerungsInfoType" minOccurs="0"/>
			<xsd:element name="FoAnkunft" type="FoAnkunftAbfahrtType" minOccurs="0"/>
			<xsd:element name="FoAbfahrt" type="FoAnkunftAbfahrtType" minOccurs="0"/>
			<xsd:element name="FoAnkunftGleichAbfahrt" type="xsd:boolean"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoAnkunftAbfahrtType">
		<xsd:sequence>
			<xsd:element name="FoFahrzeugeAmHalt">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FoFahrzeugAmHalt" type="FoFahrzeugAmHaltType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="FoSektorPositionen" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FoSektorPosition" type="FoSektorHaltPositionType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="FoFahrtrichtungAmHalt" type="FoFahrtrichtungAmHaltEnum" minOccurs="0"/>
			<xsd:element name="FoAenderungen" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FoAenderung" type="FoAenderungAmHaltType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoFahrzeugAmHaltType">
		<xsd:sequence>
			<xsd:element name="FoFahrzeugIDREF" type="FoFahrzeugIDREFType"/>
			<xsd:element name="FoHaltPosition" type="FoHaltPositionType"/>
			<xsd:element name="FoZustand" type="FoZustandType" minOccurs="0"/>
			<xsd:element name="FoErweiterung" type="ErweiterungType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoHaltPositionType">
		<xsd:annotation>
			<xsd:documentation>Entweder Sektor Bezeichnung oder Position muss angegeben werden.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FoHaltPositionStart" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Startposition des Fahrzeugs/Sektors in Prozent im Verhältnis zur gesamten Länge des Bahnsteigs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FoHaltPositionEnde" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Endposition des Fahrzeugs/Sektors in Prozent im Verhältnis zur gesamten Länge des Bahnsteigs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoSektorHaltPositionType">
		<xsd:annotation>
			<xsd:documentation>Entweder Sektor Bezeichnung oder Position muss angegeben werden.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FoSektorBezeichnung" type="FoStringType">
				<xsd:annotation>
					<xsd:documentation>Bezeichnung des Sektors  z.B. "A", "B", "1", "3", etc. .</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FoHaltPosition" type="FoHaltPositionType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoAenderungAmHaltType">
		<xsd:sequence>
			<xsd:element name="FoAenderungsCodeAmHalt" type="FoAenderungsCodeAmHaltEnum">
				<xsd:annotation>
					<xsd:documentation>Spezieller Änderungscode für Haltänderungen</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FoAenderungsTexte" type="FoAenderungsTexteType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>============================== Formation mehrfach verwendete  Typen =================================</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="FoZustandType">
		<xsd:sequence>
			<xsd:element name="FoZustandsCode" type="FoZustandsCodeEnum"/>
			<xsd:element name="FoZustandsKurzform" type="TextMitSprachCodeType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Zusammenfassung der Änderung in knapper Form</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FoZustandsText" type="TextMitSprachCodeType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Erläuterung der Änderung</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FoZustandsEmpfehlung" type="TextMitSprachCodeType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Empfehlung für Fahrgäste, wie sie mit der Änderung umgehen sollen</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FoAenderungsTexteType">
		<xsd:sequence>
			<xsd:element name="FoAenderungsKurzform" type="TextMitSprachCodeType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Zusammenfassung der Änderung in knapper Form</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FoAenderungsText" type="TextMitSprachCodeType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Erläuterung der Änderung</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FoAenderungsEmpfehlung" type="TextMitSprachCodeType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Empfehlung für Fahrgäste, wie sie mit der Änderung umgehen sollen</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="FoPositionType">
		<xsd:restriction base="xsd:integer"/>
	</xsd:simpleType>
	<xsd:simpleType name="FoBarrierefreiType">
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:simpleType name="FoStringType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="TextMitSprachCodeType">
		<xsd:simpleContent>
			<xsd:extension base="FoStringType">
				<xsd:attribute name="Sprachcode" type="xsd:language" use="optional" default="de"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="ErweiterungType">
		<xsd:complexContent>
			<xsd:extension base="xsd:anyType"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:documentation>============================== Formation ID Typen ==================================================</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="FoFahrzeugIDREFType">
		<xsd:restriction base="xsd:IDREF"/>
	</xsd:simpleType>
	<xsd:simpleType name="FoStatusIDREFType">
		<xsd:restriction base="xsd:IDREF"/>
	</xsd:simpleType>
	<xsd:simpleType name="FoFahrzeugAusstattungIDREFType">
		<xsd:restriction base="xsd:IDREF"/>
	</xsd:simpleType>
	<xsd:simpleType name="FoFahrzeugGruppeIDREFType">
		<xsd:restriction base="xsd:IDREF"/>
	</xsd:simpleType>
	<xsd:annotation>
		<xsd:documentation>============================== Formation Enumeration ===============================================</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="FoOrientierungEnum">
		<xsd:restriction base="FoStringType">
			<xsd:enumeration value="vorwaerts"/>
			<xsd:enumeration value="rueckwaerts"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FoAenderungsCodeEnum">
		<xsd:annotation>
			<xsd:documentation>-- FehlenderFamilienwagen : Zug verkehrt ohne Familienwagen 
-- FehlendeFzgGruppe : Fehlende Fahrzeuggruppe 
-- FehlendeKurswagen : Im Zug fehlen planmässige Kurswagen 
-- FehlendesRestaurant : Zug verkehrt ohne Speisewagen 
-- FehlendeWagen : Zug führt weniger Wagen als planmässig 
-- FehlendesRollstuhlplaetze : Zug verkehrt ohne Speisewagen 
-- FehlendeNiederflurwagen : Zug führt weniger Wagen als planmässig 
-- GeaenderteWagenreihung : Zug verkehrt in geänderter Formation (generelle Formationsänderung ohne genauere Spezifikation der Änderung) 
-- UmgekehrteWagenreihung : Zugformation gespiegelt (aber ohne zusätzliche/fehlende Fahrzeuge) 
-- ZusaetzlicheFzgGruppe : Zug führt zusätzliche Fahrzeuggruppe 
-- ZusaetzlicheKurswagen : Zug führt ausserplanmässige Kurswagen 
-- ZusaetzlicheWagen : Zug führt zusätzliche Wagen 
</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="FoStringType">
					<xsd:enumeration value="FehlendeFamilienwagen"/>
					<xsd:enumeration value="FehlendeFzgGruppe"/>
					<xsd:enumeration value="FehlendeKurswagen"/>
					<xsd:enumeration value="FehlendeRestaurantwagen"/>
					<xsd:enumeration value="FehlendeWagen"/>
					<xsd:enumeration value="FehlendeRollstuhlplaetze"/>
					<xsd:enumeration value="FehlendeNiederflurwagen"/>
					<xsd:enumeration value="GeaenderteWagenreihung"/>
					<xsd:enumeration value="UmgekehrteWagenreihung"/>
					<xsd:enumeration value="ZusaetzlicheFzgGruppe"/>
					<xsd:enumeration value="ZusaetzlicheKurswagen"/>
					<xsd:enumeration value="ZusaetzlicheWagen"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:union memberTypes="FoStringType"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="FoAenderungsCodeAmHaltEnum">
		<xsd:annotation>
			<xsd:documentation>-- FehlenderFamilienwagen : Zug verkehrt ohne Familienwagen 
-- FehlendeFzgGruppe : Fehlende Fahrzeuggruppe 
-- FehlendeKurswagen : Im Zug fehlen planmässige Kurswagen 
-- FehlendesRestaurant : Zug verkehrt ohne Speisewagen 
-- FehlendeWagen : Zug führt weniger Wagen als planmässig 
-- GeaenderteWagenreihung : Zug verkehrt in geänderter Formation (generelle Formationsänderung ohne genauere Spezifikation der Änderung) 
-- UmgekehrteWagenreihung : Zugformation gespiegelt (aber ohne zusätzliche/fehlende Fahrzeuge) 
-- ZusaetzlicheFzgGruppe : Zug führt zusätzliche Fahrzeuggruppe 
-- ZusaetzlicheKurswagen : Zug führt ausserplanmässige Kurswagen 
-- ZusaetzlicheWagen : Zug führt zusätzliche Wagen 
</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="FoStringType">
					<xsd:enumeration value="FehlendeFamilienwagen"/>
					<xsd:enumeration value="FehlendeFzgGruppe"/>
					<xsd:enumeration value="FehlendeKurswagen"/>
					<xsd:enumeration value="FehlendeRestaurantwagen"/>
					<xsd:enumeration value="FehlendeWagen"/>
					<xsd:enumeration value="GeaenderteWagenreihung"/>
					<xsd:enumeration value="UmgekehrteWagenreihung"/>
					<xsd:enumeration value="ZusaetzlicheFzgGruppe"/>
					<xsd:enumeration value="ZusaetzlicheKurswagen"/>
					<xsd:enumeration value="ZusaetzlicheWagen"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:union memberTypes="FoStringType"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="FoZustandsCodeEnum">
		<xsd:annotation>
			<xsd:documentation>
-- defekt : Ausstattung defekt 
-- geschlossen : Ausstattung/Fahrzeug geschlossen 
-- nicht_verfuegbar : Ausstattung nicht verfügbar 
-- offen : Ausstattung/Fahrzeug offen 
-- reserviert : Ausstattung reserviert  
-- nicht_bedient : Fahrzeug offen, aber Restaurant unbedient 
-- verfuegbar : Ausstattung verfügbar 
</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="FoStringType">
					<xsd:enumeration value="defekt"/>
					<xsd:enumeration value="geschlossen"/>
					<xsd:enumeration value="nicht_verfuegbar"/>
					<xsd:enumeration value="offen"/>
					<xsd:enumeration value="reserviert"/>
					<xsd:enumeration value="nicht_bedient"/>
					<xsd:enumeration value="verfuegbar"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:union memberTypes="FoStringType"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="FoBeziehungsTypEnum">
		<xsd:annotation>
			<xsd:documentation>
-- FortfuehrungDurchFahrt : Fortfuehrung der Fahrt Durch Fahrt x 
-- FortfuehrungVonFahrt : Fortfuehrung von Fahrt x 
-- TrennungVonFahrtIn : Trennung der Fahrt in die Fahrten xy 
-- FortfuehrungVonGetrennterFahrt: Fortführung von getrennter Fahrt x 
-- ZusammenfuehrungVonFahrt : Zusammenfuehren der Fahrte x und y in xy 
-- FortfuehrungDurchZusammengefuehrteFahrt : Dies Fahrt wird durch die zusammengefuehrte Fahrt x fortgesetzt 
-- ErsatzVonFahrt : Diese Fahrt ersetzt Fahrt x 
-- ErsatzDurchFahrt : Diese Fahrt wird ersetzt durch Fahrt x 
-- EntlastungVonFahrt : Diese Fahrt ersetzt Fahrt x 
-- EntlastungDurchFahrt : Diese Fahrt wird durch Fahrt x ersetzt 
</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="FoStringType">
					<xsd:enumeration value="FortfuehrungDurchFahrt"/>
					<xsd:enumeration value="FortfuehrungVonFahrt"/>
					<xsd:enumeration value="TrennungVonFahrtIn"/>
					<xsd:enumeration value="FortfuehrungVonGetrennterFahrt"/>
					<xsd:enumeration value="ZusammenfuehrungVonFahrt"/>
					<xsd:enumeration value="FortfuehrungDurchZusammengefuehrteFahrt"/>
					<xsd:enumeration value="ErsatzVonFahrt"/>
					<xsd:enumeration value="ErsatzDurchFahrt"/>
					<xsd:enumeration value="EntlastungVonFahrt"/>
					<xsd:enumeration value="EntlastungDurchFahrt"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:union memberTypes="FoStringType"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="FoFahrtrichtungAmHaltEnum">
		<xsd:restriction base="FoStringType">
			<xsd:enumeration value="gegen0"/>
			<xsd:enumeration value="gegen100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FoFahrzeugAusstattungsCodeEnum">
		<xsd:annotation>
			<xsd:documentation>
-- AbteilBusiness : Fahrzeug mit Businessabteil 
-- AbteilFahrrad : Fahrradabteil vorhanden 
-- AbteilFahrradResPflicht : Fahrradabteil vorhanden mit Reservationspflicht 
-- AbteilKleinkind : Fahrzeug mit Kleinkinderabteil 
-- AbteilRollstuhl : Fahrzeug mit Abteil für Rollstuhlfahrer 
