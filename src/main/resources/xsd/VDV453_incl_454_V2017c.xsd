		<xsd:sequence>
			<xsd:element name="LinienFilter" type="LinienFilterType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="BetreiberFilter" type="BetreiberFilterType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="ProduktFilter" type="ProduktFilterType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="VerkehrsmittelTextFilter" type="VerkehrsmittelTextFilterType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="HaltFilter" type="HaltFilterType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="UmlaufFilter" type="UmlaufFilterType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="UmlaufID" type="UmlaufIDType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Veraltet. Soll nicht benutzt werden.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Hysterese" type="HystereseType"/>
			<xsd:element name="Vorschauzeit" type="VorschauzeitType"/>
			<xsd:element name="MitGesAnschluss" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="MitRealZeiten" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="MitFormation" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="NurAktualisierung" type="xsd:boolean" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="AboID" type="AboIDType" use="required"/>
		<xsd:attribute name="VerfallZst" type="xsd:dateTime" use="required"/>
	</xsd:complexType>
