			<xsd:element name="AbfahrtszeitASBPlan" type="xsd:dateTime"/>
			<xsd:element name="HaltID" type="HaltIDType" minOccurs="0"/>
			<xsd:element name="AbfahrtssteigText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="HaltepositionsText" type="HaltepositionsTextType" minOccurs="0"/>
			<xsd:element name="AbfahrtsSektorenText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="FahrtInfo" type="FahrtInfoType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="Abbringernachricht" type="AbbringernachrichtType"/>
