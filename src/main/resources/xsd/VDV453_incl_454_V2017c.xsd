			<xsd:element name="DatenVorhandenBis" type="xsd:dateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Veraltet. Soll nicht benutzt werden.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
