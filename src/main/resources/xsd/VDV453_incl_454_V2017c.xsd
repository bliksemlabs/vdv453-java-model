-- WLAN : WLAN Hotspot vorhanden 
</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="FoStringType">
					<xsd:enumeration value="AbteilBusiness"/>
					<xsd:enumeration value="AbteilFahrrad"/>
					<xsd:enumeration value="AbteilFahrradResPflicht"/>
					<xsd:enumeration value="AbteilKleinkind"/>
					<xsd:enumeration value="AbteilRollstuhl"/>
					<xsd:enumeration value="Klima"/>
					<xsd:enumeration value="Plaetze1"/>
					<xsd:enumeration value="Plaetze2"/>
					<xsd:enumeration value="PlaetzeCC"/>
					<xsd:enumeration value="PlaetzeFahrrad"/>
					<xsd:enumeration value="PlaetzeFahrradResPflicht"/>
					<xsd:enumeration value="PlaetzeRollstuhl"/>
					<xsd:enumeration value="PlaetzeSteh"/>
					<xsd:enumeration value="PlaetzeWL"/>
					<xsd:enumeration value="PlaetzeWR"/>
					<xsd:enumeration value="RollstuhlToilette"/>
					<xsd:enumeration value="Ruhe"/>
					<xsd:enumeration value="Toilette"/>
					<xsd:enumeration value="WLAN"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:union memberTypes="FoStringType"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:simpleType name="FoTechnischesAttributCodeEnum">
		<xsd:annotation>
			<xsd:documentation>
-- AnzAchsen : Anzahl Achsen 
-- FzGesamtLaenge : Gesamtlänge des Fahrzeugs, z.B. Länge über Puffer in Meter 
-- FzUICAustauschregime  : Ziffer 1 und 2 der UIC Wagennummer: Austauschregime 
-- FzUICNummer : Vollständige UIC Wagennummer (12 Ziffern) 
-- FzUICOrdnungsnummer : Ziffer 9 bis 11 der UIC Wagennummer: Ordnungsnummer 
-- FzUICPruefziffer : Ziffer 12 der UIC Wagennummer: Prüfziffer 
-- FzUICTyp : Ziffer 5 bis 8 der UIC Wagennummer: Gattungskennzahlen und Heizsystem 
-- FzUICVerwaltung : Ziffer 3 und 4 der UIC Wagennummer: Ländercode 
-- GewichtLeer : Leergewicht in Tonnen 
-- HoeheEinstieg : Einstiegshöhe über Schienenoberkante in Zentimeter 
-- Stufenfrei : Fahrzeug ohne Stufen zwischen Trittbrett und Fahrgastraum 
-- NiederflurEinstieg : Fahrzeug mit Niederflureinstieg
</xsd:documentation>
		</xsd:annotation>
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="FoStringType">
					<xsd:enumeration value="AnzAchsen"/>
					<xsd:enumeration value="FzGesamtLaenge"/>
					<xsd:enumeration value="FzUICAustauschregime"/>
					<xsd:enumeration value="FzUICNummer"/>
					<xsd:enumeration value="FzUICOrdnungsnummer"/>
					<xsd:enumeration value="FzUICPruefziffer"/>
					<xsd:enumeration value="FzUICTyp"/>
					<xsd:enumeration value="FzUICVerwaltung"/>
					<xsd:enumeration value="GewichtLeer"/>
					<xsd:enumeration value="HoeheEinstieg"/>
					<xsd:enumeration value="Stufenfrei"/>
					<xsd:enumeration value="NiederflurEinstieg"/>
				</xsd:restriction>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:union memberTypes="FoStringType"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>
	<xsd:annotation>
		<xsd:documentation>============================== Fahrtverband ======================================================</xsd:documentation>
	</xsd:annotation>
	<xsd:complexType name="FahrtVerbandType">
		<xsd:sequence>
			<xsd:element name="BetrieblicheFahrtnummer" type="xsd:string" minOccurs="0">
				<xsd:annotation>
