			<xsd:element name="ASBID" type="IDType"/>
			<xsd:element name="AbbringerInfo" type="AbbringerInfoType"/>
			<xsd:element name="FahrtIDExt" type="FahrtIDExtType" maxOccurs="unbounded"/>
			<xsd:element name="Ursache" type="UrsacheType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="Zst" type="xsd:dateTime" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="AbbringerInfoType">
		<xsd:sequence>
			<xsd:element name="FahrtID" type="FahrtIDType"/>
